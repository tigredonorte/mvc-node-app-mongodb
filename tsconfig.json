{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "rootDir": "./src",
    "outDir": "./dist",
    "baseUrl": ".",
    "types": [ "node" ],
    "typeRoots": ["./src/types"],
    "paths": {
      "~*": [
        "./src/*"
      ],
      "@env": [
        ".env",
      ]
    },
  },
  "exclude": ["./node_modules"]
}
